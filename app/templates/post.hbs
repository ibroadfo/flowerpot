{{partial 'otherHeader'}}
{{#each flashMessages.queue as |flash|}}
  {{#flash-message flash=flash as |component flash|}}
    <span id='new-post-header'>{{flash.message}}
      {{twitter-share text=model.tweetString}}
    </span>
  {{/flash-message}}
{{/each}}
<main role="main" id="main">
{{post-post model=model}}
</main>
